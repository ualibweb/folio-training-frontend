  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "classnames": "^2.3.1",
    "final-form": "^4.20.7",
    "final-form-arrays": "^3.1.0",
    "history": "^4.9.0",
    "ky": "^0.31.1",
    "react-router": "^6.12.1",
    "utility-types": "^3.10.0"
  },
  "resolutions": {
    "@types/react": "^17.0.2"
  },
  "peerDependencies": {
    "@folio/stripes": "^8.0.0",
    "@types/react": "^17.0.2",
    "final-form": "^4.20.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.9",
    "react-intl": "^5.7.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^5.2.0"
  },
  "stripes": {
    "actsAs": [
      "app"
    ],
    "displayName": "ui-training.meta.title",
    "route": "/training",
    "hasSettings": false,
    "okapiInterfaces": {},
    "permissionSets": [
      {
        "permissionName": "module.training.enabled",
        "displayName": "UI: Training module is enabled",
        "visible": true
      }
    ]
  }
